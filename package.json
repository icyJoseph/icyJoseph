{
  "name": "icyjoseph",
  "homepage": "https://icyJoseph.github.io/",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@egoist/snackbar": "^1.2.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-brands-svg-icons": "^5.6.3",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "axios": "^0.19.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.0",
    "react-dom": "^16.8.6",
    "react-loadable": "^5.4.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.0",
    "react-scripts": "^3.1.2",
    "redux": "^4.0.4",
    "redux-saga": "^1.0.0",
    "styled-components": "^4.1.2",
    "use-easing": "^0.0.2"
  },
  "scripts": {
    "analyze": "yarn build && source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build && cp build/index.html build/404.html",
    "test": "react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom --watchAll --coverage",
    "test:report": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -b master -d build",
    "production": "yarn test:report && yarn deploy"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "gh-pages": "^1.1.0",
    "react-testing-library": "^7.0.0",
    "redux-mock-store": "^1.5.1",
    "source-map-explorer": "^2.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/registerServiceWorker.js",
      "!src/index.js",
      "!src/ducks/store/**.js",
      "!src/setupTests.js",
      "!src/data/*.js"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "resolutions": {
    "browserslist": "4.6.2",
    "caniuse-lite": "1.0.30000974"
  }
}
