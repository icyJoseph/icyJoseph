{
  "name": "icyjoseph",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "react": "^16.3.2",
    "react-animate-height": "^1.0.2",
    "react-countup": "^3.0.3",
    "react-dom": "^16.3.2",
    "react-hexagon": "^1.1.3",
    "react-loadable": "^5.4.0",
    "react-media": "^1.8.0",
    "react-redux": "^5.0.7",
    "react-reveal-text": "^0.1.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.79.1",
    "styled-components": "^3.2.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom --watchAll --coverage",
    "test:coverage": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "gh-pages": "^1.1.0",
    "redux-mock-store": "^1.5.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/registerServiceWorker.js",
      "!src/index.js",
      "!src/ducks/store/**.js",
      "!src/setupTests.js"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    }
  }
