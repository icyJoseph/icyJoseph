{
  "name": "icyjoseph",
  "homepage": "https://icyJoseph.github.io/",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.2",
    "next": "^9.5.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "swr": "^0.2.3",
    "use-easing": "^0.0.2"
  },
  "scripts": {
    "analyze": "node analyze.js",
    "source-map-explore": "yarn build && source-map-explorer 'build/static/js/*.js'",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --watch",
    "test:ci": "jest --ci"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.1.0",
    "@testing-library/react": "^9.4.0",
    "babel-jest": "^25.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "progress-bar-webpack-plugin": "^1.12.1",
    "react-test-renderer": "^16.12.0",
    "react-testing-library": "^7.0.0",
    "source-map-explorer": "^2.0.1",
    "webpack-bundle-analyzer": "^3.5.2"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "react/self-closing-comp": [
        "error",
        {
          "component": true,
          "html": true
        }
      ]
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/registerServiceWorker.js",
      "!src/index.js",
      "!src/ducks/store/**.js",
      "!src/setupTests.js",
      "!src/data/*.js"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
